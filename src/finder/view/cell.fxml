<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
  prefHeight="80.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1"
  xmlns="http://javafx.com/javafx/2.2">
  <center>
    <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
      <children>
        <Pane prefHeight="40.0" prefWidth="521.0" VBox.vgrow="ALWAYS">
          <children>
            <Label fx:id="lb_name" text="lb_name" textFill="BLACK"/>
          </children>
          <padding>
            <Insets/>
          </padding>
          <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" fx:id="x2"/>
          </VBox.margin>
        </Pane>
        <HBox alignment="BOTTOM_LEFT" prefHeight="30.0" prefWidth="521.0" VBox.margin="$x2"
          VBox.vgrow="ALWAYS">
          <children>
            <BorderPane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
              <left>
                <Label fx:id="lb_created" text="lb_created">
                  <textFill>
                    <Color blue="0.375" green="0.375" red="0.375" fx:id="x1"/>
                  </textFill>
                </Label>
              </left>
              <right>
                <Label fx:id="lb_size" text="lb_size" textFill="$x1"/>
              </right>
            </BorderPane>
          </children>
        </HBox>
      </children>
    </VBox>
  </center>
  <left>
    <ImageView fx:id="iv_thumbnail" fitHeight="81.0" fitWidth="79.0" pickOnBounds="true"
      preserveRatio="true" BorderPane.alignment="CENTER"/>
  </left>
</BorderPane>
